@model OnlineExam.Models.mcqQS

@{
    ViewData["Title"] = "Exam";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h4>mcqQS</h4>
    <hr />
    @using (Html.BeginForm("Next", "mcqQS", FormMethod.Post))
{ 
    <dl class="row">
        
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.qsNo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.qsNo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.qs)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.qs)
        </dd>
        <dt class="col-sm-2">
            @Html.RadioButton("Options", 1, true)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.op1)
        </dd>
        <dt class="col-sm-2">
            @Html.RadioButton("Options", 2, true)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.op2)
        </dd>
        <dt class="col-sm-2">
            @Html.RadioButton("Options", 3, true)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.op3)
        </dd>
        <dt class="col-sm-2">
            @Html.RadioButton("Options", 4, true)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.op4)
        </dd>
        
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.eTime)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.eTime)
        </dd>
    </dl>
        <input type="submit" value="submit" />
    }
</div>
